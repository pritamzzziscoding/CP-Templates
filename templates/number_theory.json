        "prefix": "number_theory",
        "body": [
            "// O(sqrt n) using Trial Division",
            "template<class U>",
            "vector<U> getPrimeFactors(U n){",
            "    vector<U> pf;",
            "    for (U i = 2; i * i <= n; i++){",
            "        while(n % i == 0) {",
            "            pf.push_back(i);",
            "            n /= i;",
            "        }",
            "    }",
            "    if(n > 1) pf.push_back(n);",
            "    return pf;",
            "}",
            "",
            "template<class T>",
            "vector<int> getDivisors(T n){",
            "    vector<int> divisors;",
            "    for(int i = 1; i * i <= n; i++){",
            "        if(n % i == 0){",
            "            divisors.push_back(i);",
            "            if(1LL * i * i != n){",
            "                divisors.push_back(n / i);",
            "            }",
            "        }",
            "    }",
            "    sort(divisors.begin(), divisors.end());",
            "    return divisors;",
            "}",
            "",
            "template<class T>",
            "T lcm(T a, T b){",
            "    return (a / __gcd(a, b)) * b;",
            "}"
        ],
        "description": "Number Theory (Trial Prime Factorizaton, Divisors, LCM)"
    }
}