            "const int PRIME_MAXN = 1000001;",
            "int prime[PRIME_MAXN];",
            "void eratosthenes(){",
            "    for(int i = 0; i < PRIME_MAXN; i++) prime[i] = 1;",
            "    prime[0] = prime[1] = 0;",
            "    for(int i = 2; i * i < PRIME_MAXN; i++){",
            "        if(prime[i] == 1){",
            "            for(int j = i * i; j < PRIME_MAXN; j += i){",
            "                prime[j] = 0;",
            "            }",
            "        }",
            "    }",
            "}",
            "bool checkprime(int n){",
            "    if(n < 0 || n >= PRIME_MAXN) return false;",
            "    return prime[n];",
            "}",
